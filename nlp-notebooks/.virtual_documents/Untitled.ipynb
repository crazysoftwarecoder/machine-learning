"""


print(corpus)


## Tokenize sentences
from nltk.tokenize import sent_tokenize


documents = sent_tokenize(corpus)


type(documents)


for sent in documents:
    print(sent)


## Tokenization -> sentence to words
from nltk.tokenize import word_tokenize


word_tokenize(corpus)



